{
    "name": "tic-tac-toe",
    "root": "apps/tic-tac-toe",
    "sourceRoot": "apps/tic-tac-toe/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "rsbuild build",
                "cwd": "apps/tic-tac-toe"
            }
        },
        "dev": {
            "executor": "nx:run-commands",
            "options": {
                "command": "rsbuild dev --open",
                "cwd": "apps/tic-tac-toe"
            }
        }
    }
}